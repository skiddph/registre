{
    "name": "registre",
    "private": true,
    "version": "2.0.0",
    "entry": "index.js",
    "scripts": {
        "start": "cross-env NODE_ENV=production node index.js",
        "dev": "cross-env NODE_ENV=development VITE_API_URL=http://127.0.0.1:3000/api vite --port 80 --host --https",
        "build": "vite build",
        "preview": "vite preview --host",
        "api": "cross-env PORT=3000 NODE_ENV=development nodemon index.js",
        "serve": "cross-env NODE_ENV=production vite build && node index.js",
        "setup": "cross-env NODE_ENV=production && npx prisma migrate dev -n init && npx prisma migrate deploy && npx prisma db push",
        "reset": "npx prisma migrate reset --force"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.3.0",
        "@fortawesome/free-solid-svg-icons": "^6.0.0",
        "@fortawesome/vue-fontawesome": "^3.0.0-5",
        "@prisma/client": "^3.10.0",
        "bcrypt": "^5.0.1",
        "date-fns": "^2.28.0",
        "dotenv": "^14.3.2",
        "fastify": "^3.27.0",
        "fastify-cors": "^6.0.2",
        "fastify-https-redirect": "^1.0.4",
        "fastify-jwt": "^4.1.3",
        "fastify-plugin": "^3.0.1",
        "fastify-static": "^4.5.0",
        "lodash": "^4.17.21",
        "postcss-import": "^14.0.2",
        "qrcode-reader-vue3": "^3.1.2",
        "usid": "^1.0.21",
        "vue": "^3.2.25",
        "vue-router": "^4.0.12"
    },
    "devDependencies": {
        "@vitejs/plugin-vue": "^2.0.0",
        "autoprefixer": "^10.4.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.5.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-vue": "^8.2.0",
        "nodemon": "^2.0.15",
        "postcss": "^8.4.5",
        "prettier": "^2.5.1",
        "prisma": "^3.10.0",
        "sass": "^1.45.1",
        "tailwindcss": "^3.0.7",
        "vite": "^2.7.2",
        "vite-plugin-md": "^0.11.7",
        "vite-plugin-pages": "^0.20.0",
        "vite-plugin-vue-layouts": "^0.5.0",
        "vuex": "^4.0.2"
    },
    "engine": {
        "node": ">=12.0.0"
    }
